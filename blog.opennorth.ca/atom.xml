---
# https://raw.github.com/plusjade/jekyll-bootstrap/master/atom.xml
layout: nil
title: Open North Blog Feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xml:lang="en" xmlns="http://www.w3.org/2005/Atom">
  <id>tag:blog.opennorth.ca,2005:/</id>
  <link rel="alternate" type="text/html" href="http://blog.opennorth.ca"/>
  <link rel="self" type="application/atom+xml" href="http://blog.opennorth.ca/atom.xml"/>
  <title>Open North Blog</title>
  <author>
    <name>Open North</name>
  </author>
  <updated>{{ site.time | date_to_xmlschema }}</updated>

{% for post in site.posts %}
  <entry>
    <id>tag:blog.opennorth.ca,2005:{{ post.id }}</id>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <link rel="alternate" type="text/html" href="http://blog.opennorth.ca{{ post.url | pretty_url }}"/>
    <author>
      <name>{{ post.author }}</name>
    </author>
    <title>{{ post.title }}</title>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
{% endfor %}
</feed>